namespace selium.net;

table NetReader {
  shared_handle: ulong;
  protocol: ubyte;
  chunk_size: uint;
}

table NetWriter {
  shared_handle: ulong;
  protocol: ubyte;
}

table NetConnection {
  reader: NetReader;
  writer: NetWriter;
  remote_addr: string;
}

root_type NetConnection;
file_identifier "SNET";
