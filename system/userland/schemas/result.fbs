// Flatbuffers schema for generic success/error envelopes.
namespace selium.result;

file_identifier "SRES";

/// Success/error result carrying a payload or error string.
table FlatResult {
  /// Correlation identifier (zero when not used).
  request_id:ulong;
  /// Raw payload bytes.
  payload:[ubyte];
  /// Error string populated when representing failure.
  error:string;
}

root_type FlatResult;
